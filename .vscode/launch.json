    "version": "0.2.0",
    "configurations": [
        {
            "command": "npm run dev",
            "name": "web server start",
            "request": "launch",
            "cwd": "${workspaceFolder}/web",
            "type": "node-terminal"
        },
        {
            "name": "api server start",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/src",
            "program": "${workspaceFolder}/src/app.py",
            "console": "integratedTerminal"
        },
